<h1>New Category</h1>

<%= form_with model: @group, url: user_groups_path do |form| %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :icon %>
    <%= form.text_field :icon %>
  </div>

  <%= form.submit "Create Category" %>
<% end %>

<%= link_to "Back to categories", root_path %>
